/* disposition des elements */

body {
	margin:0;
	padding:0;
	font-family:tahoma,verdana,arial;
	font-size:10pt;
	
}

#portlet-col-g {
	float:left;
	width:144px;
  padding:0.2em;
  margin:0;
  vertical-align:top;
/*  background-color:#fcfcfc;*/
/*	border-right: 1px solid #f0f0f0;*/
}

#portlet-col-d {
  text-align:left;
  vertical-align:top;
  background-color:white;
  margin:0;
  padding-left:150px;  
}

#portlet-content {
	padding:0.5em;
	/*padding:0;*/
	margin:0;
	position:relative;
}

form {
	margin:0;
	padding:0;
}


.br {
  clear:both;
  display:block;
  margin:0;
  padding:0;
  height:20px;
  line-height:0px;
  font-size:0px;
}



hr {
	 border:none;
	 border-top:#c1d2ee 1px solid;
	 height:1px;
}

fieldset {
border:#000 1px solid;
margin: 0em 0em 1em 0em;
padding: 0em 1em 1em 1em;
}

legend {
background: #FFF;
padding-left: 0.5em;
padding-right: 0.5em;
font-size: 10pt;
font-weight:bold;
}

#command .action {
	margin-top:1em;
	text-align:left;
	padding:0.5em 0.5em 0.5em 0.5em;
	background-color:#F9F9F9;
	/*border-top:#d9c55a 1px solid;*/
	background-color:#e9edf2;
	border:#c1d2ee 1px solid;	
}


a {
	text-decoration:none;
	color:black;
	font-family:Verdana,Tahoma,Arial,Times;
	font-size:11px;	
}
a:hover{
	text-decoration:underline;
	color:#E67D02;
}


select {
	
	font-family:Tahoma,verdana;
	font-size:12px;		
	
 background:#fff url(../images/bb.gif) repeat-x 0 0;
 height:16px;
 line-height:18px;
 background-color:#fff; /* opera */
 border: 1px solid #B5B8C8;		
	
}

input.text {
	font-family:Tahoma,verdana;
	font-size:12px;
	/* border:#eee 1px solid;*/
 padding: 1px 3px;
 background:#fff url(../images/bb.gif) repeat-x 0 0;
 border: 1px solid #B5B8C8;
 width:200px;	
}


input.text:focus, select:focus {
	border:#E67D02 1px solid;
}

.label,.label1 {
		font-weight: bold;
		font-size:11px;		
		font-family:Verdana;
		padding-right:0.5em;
		padding-left:0.5em;		
	
}

.label1 {
float: left;	
	width: 80px;	
	text-align: left;	
	margin: 0;
	padding: 0 .5em 0 0;
	padding-left:0.5em;
	line-height: 1.8;
}

div .navi {
	padding:0.5em;
	background-color:#e9edf2;
	border:#c1d2ee 1px solid;
	
	vertical-align:middle;
	/*
	line-height:20px;
	height:20px;
	*/
	
}

div .navi a {
	font-size:10pt;
}

.listeR {
 width:100%;
}

.listeR td {
	padding:2px;
	font-family:tahoma,verdana,arial;
	font-size:12px;
}


.pair {
background-color: #F9F9F9;
}

.pair:hover {
	#background-color: #fffcd0;;
}

.uvs {
	width:85px!important;
	}

/* -------------------------- */

.recherche {
	font-family:Verdana;
	font-size:10px;
	background: transparent url(../images/fond-filtre.png) top left repeat-x;
	margin:0;
	padding:0;
	height:80px;
	padding-left:15px;
}


/*
select {
	font-family:Tahoma,verdana;
	font-size:11px;		
}
*/
h5 {
	background-color:transparent;
	margin:0;
	font-family:Tahoma,Verdana;
	font-size:12px;		
	font-weight:bold;
	padding:0.5em 0 0.5em 0;
}


/* -- */

.resume {
	font-family:Tahoma,verdana;
	font-size:11px;		
	 margin:0;
	 padding:0;
	 border-collapse:collapse;
	 border:#D9D9D9 1px solid;
}


.resume TD {
  vertical-align:middle;
  margin:0;
  padding:2px;
  border-bottom:#D9D9D9 1px solid;
}

H4 {
  background:transparent url(../images/pucetitre.gif) no-repeat left center;
  padding-left:10px;
  margin-top:10px;
  margin-bottom:10px;
}

H4 A.lienh4,  H4 A.lienh4:visited {
  font-size:10pt;
  line-height:12pt;
  background-color:white;
  text-decoration:none;
  color:black;
}


H4 A.lienh4:hover {
  font-size:10pt;
  line-height:12pt;
  color:black;
  background-color:#E9E9E9;
  text-decoration: none;
}


.hide {
  display:none;
}

.show {
  display:block;
}


.legende {
	padding-left:10px;
  font-size:10pt;
	font-weight: normal;
	padding-bottom:10px;
}

.legende span {
	/*border:1px solid #D9D9D9;*/
}

/* table profil */
.profil {
 margin:0;
 padding:0;
 border-collapse:collapse;
 font-family:Tahoma,verdana;
 font-size:11px;		

}


.profil TD {
  border:#D9D9D9 1px solid;
  vertical-align:top;
  margin:0;
  padding:2px;
}


.profil TD table {
 width:100%!important;
 height:100%!important;
 border-collapse:collapse;
}

.profil TD table TD {
  border:0;
  margin:0;
  
  padding:2px;
}

/* ligne alternées */
.odd, .odd-c {
 background-color:#F9F9F9;
 background-color:#f3f4f8;
}

.even, .even-c {
 background-color:#E9E9E9;
}

/* coin superieur gauche */
.coin-sg {
  border:0!important;
  border-bottom:#D9D9D9 1px solid;
  background-color:#FFF;
  width:80px;
}

/* coin inférieur droit */
.coin-id {
  border:0;
  border-bottom:#D9D9D9 1px solid;
  background-color:#FFF;

}

.entete-haut {
  border:#D9D9D9 1px solid;
  width:90px;
  background-color:#E9E9E9;
  background-color:#edeef4;
}

.sem {
	width:95px!important;
	white-space:nowrap;
}

.observation {
	width:250px;
}

.even TD, .odd TD, .total, .sstotal {
  border-top:#D9D9D9 1px solid;
  border-left:#D9D9D9 1px solid;
}



.stotal {
	text-align:right;
}

.lib-stotal {
	text-align:left;
}
.totals {
	text-align:left;
}


.valtotal {
	text-align:center;
	vertical-align: middle!important;
}
.lib-tot {
  vertical-align:middle!important;
  font-weight: bold;
}

.ent-categ {
/*  background:white url(../images/ent.png) top left repeat-x;*/
  background-color:#edeef4;
/*  background-color:#FFFFD6; */
  width:85px!important;
}

.acquis {
	background-color: #e3ffd5;
}

.vide {
	border:0!important;
}

.valtotal img {
	padding-left:5px;
	display:inline;
}

.message {
	background-color:#f3e4a6;
	border:#f09325 1px solid;
	font-family:Tahoma,verdana;
	font-size:11px;		
	padding:1em;
	margin:10px;
	width:50%;
}

