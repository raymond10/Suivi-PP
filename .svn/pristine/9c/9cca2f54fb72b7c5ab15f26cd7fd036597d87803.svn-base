color:#000000;
}


.symb2 {
	color:#12A267;
}

.symb {
	font-family:Tahoma,"Lucida Console",Verdana,Arial,Times!important;
	font-size:14px !important;
	font-weight:bold !important;
	margin-top:-5px!important;

}

.reoriente {
	background-color:#F5F5F5;
}

.decision {
	padding-top:0.2em;
	text-align:right;
}

/* -- */


.tip-uv {
	font-family: Verdana, Tahoma, Arial, Times;

}

.tip-uv h3 {
	font-family: Verdana, Tahoma, Arial, Times;
	margin:0;
	padding:0;
	line-height:normal;
}

.tip-p {
	margin:0;
	padding-left:0.5em;
}

.tip-txt, .tip-p {
	font-family: Verdana, Tahoma, Arial, Times;
	font-size: 11px;
	text-align: :justify!important;
}


.warn {
	background:transparent url(../images/warning.gif) top left no-repeat;
	padding-left:20px;
}
/* -- */

.form {
	width:98%;
}

.rechercheUV {
	font-family:Verdana;
	font-size:10px;
	margin:0;
	padding:0;
	height:80px;
	padding-left:15px;
	display:block;
}

.rechercheUV .form-s {
	background-color:#fbf8ee;
	padding-top:0;
	margin-top:0;
}

.rechercheUV .label, .rechercheUV .label1 {
	font-weight: bold;
	font-size:11px;		
	font-family:Verdana;		
}

.rechercheUV .label {
	float:left;
	width:10%;
	padding-left:0.5em;	
}

.rechercheUV select {
	font-family:Tahoma,verdana;
	font-size:11px;		
}

.rechercheUV h5 {
	background-color:#8397AE;
	color:#E8E4E3;
	margin:0;
	font-family:Tahoma,Verdana;
	font-size:12px;		
	font-weight:bold;
	padding:0.2em 0.5em 0.2em 0.5em;
	margin:0.2em 0 0.2em 0;
}

.rechercheUV h5 a.ah5 {
	color:#E8E4E3;
	text-decoration: none;
	font-size:12px;		
	font-weight:bold;	
}

.rechercheUV .item {
	margin:1.5em 0 1.5em 0;
	margin:0;
	padding:0;
	padding:1em 0 1em 0;
	background-color: transparent;
}

.rechercheUV .mes-aide {
	font-style: italic;
	color:#A0A0A0;
  font-family: Verdana, Tahoma, Arial, Times;
/*  padding-left:7.5em;*/
  
}

.rechercheUV .maj {
	text-transform: uppercase;
}

.rechercheUV input.saisie, .rechercheUV select.llov {
	color: Black;
	font-size: 11px;
	font-family: Verdana, Tahoma, Arial, Times;
	background:white url(../images/input_background.gif) top left repeat-x ;
	border:#C0C0C0 1px solid;
}


.rechercheUV input.saisie:focus, .rechercheUV select.llov:focus {
	border:#E67D02 1px solid;
}

.rechercheUV .erreur {
	color:red;
	font-size: 12px;
	font-family: Verdana, Tahoma, Arial, Times;
}

.rechercheUV .action {
	text-align:left;
	padding:0.5em 0.5em 0.5em 0.5em;
	background-color:#F9F9F9;
	border-top:#d9c55a 1px solid;
}

.rechercheUV .action .bouton {
	background-color:#8397AE;
	color:#E8E4E3;
	border:0;
	font-family: Verdana, Tahoma, Arial, Times;
	font-size:12px;
	font-weight:bold;
	padding:1px;
	cursor:pointer;
}

.rechercheUV .action a {
	font-family: Verdana, Tahoma, Arial, Times;
	font-size:11px;
	text-decoration: none;
	color:#8397AE;
	cursor:pointer;
}

.rechercheUV .action a:hover {
	color:#E67D02;
}

.abc {
	margin-left:2em;
	text-align:left;
}

.abc a {
	font-family: Arial, Verdana, Tahoma, Times;
	font-size:12px;
	font-weight:bold;
	text-decoration: none;
	color:#000;
	cursor:pointer;
	padding:0 0.3em 0 0.3em;
	background-color: #f9edbe;
}

.abc a:hover {
	color:#E8E4E3!important;
	background-color: #8397AE!important;
}

.info {
	background:white url(../images/help.png) no-repeat top left;
	border-bottom:#c1d2ee 1px dashed;
	color:black;
	font-weight:normal;
	font-family:Verdana,Arial,Tahoma;
	font-size:11px;
	padding: 0 0 5px 20px;
	margin:5px 10px 15px 5px;
	text-align: justify;
}

.rechercheUV .navi {
	text-align:left;
	width:97%;
	font-family:Verdana,Tahoma,Arial,Times;
	font-size:11px;
	text-decoration:none;	
}

.rechercheUV .navi span {
	text-align:left;

}

.rechercheUV .navi a {
	cursor:pointer;
	font-family:Verdana,Tahoma,Arial,Times;
	font-size:10pt;
	text-decoration:none;
	color:#8397AE;
}

.rechercheUV .navi a:hover {
	text-decoration:underline;
	color:#E67D02;
}

.rechercheUV .navi a:visited {
	text-decoration:none;
	color:black;
}

.listeUV {
	font-family:Verdana,Tahoma,Arial,Times;
	font-size:11px;	
	width:98%;
}

.listeUV tr td {
	border-bottom:1px dashed #CCCCCC;
	padding-bottom:4px;
	padding-top:4px;
}
.listeUV tr.last td {
	border-bottom:0;
	padding-bottom:0;
}

.listeUV .laction {
	background-color: #F9F9F9;
	border-right:1px solid #D9C55A;
	width:18px;
	vertical-align: top;
	text-align:center;
}

.listeUV .contenu {
	padding-left:5px;
	text-align: justify;
	
}



/* ------------------------------------------ */


.eXtremeTable {
	margin: 0;
	padding: 0;
}

.eXtremeTable select {
	font-family: Verdana;
	font-size: 9px;
	border: solid 1px #EEE;
	width: 75px;
}

.eXtremeTable .tableRegion {
	border: 1px solid silver;
	padding: 2px;
	font-family:Verdana;
	font-size: 10px;
	margin-top: 7px;
}


.eXtremeTable .filter {
	background-color: #efefef;
}

.eXtremeTable .filter input {
	font-family: Verdana;
	font-size: 10px;
	width: 100%;
}

.eXtremeTable .filter select {
	font-family: Verdana;
	font-size: 9px;
	border: solid 1px #EEE;
	width: 100%;
}

.eXtremeTable .tableHeader {
	background-color: #308dbb;
	color: white;
	font-family:Verdana;
	font-size: 11px;
	font-weight: bold;
	text-align: left;
	padding-right: 3px;
	padding-left: 3px;
	padding-top: 4;
	padding-bottom: 4;
	margin: 0;
	border-right-style: solid;
	border-right-width: 1px;
	border-color: white;
}

.eXtremeTable .tableHeaderSort {
	background-color: #3a95c2;
	color: white;
	font-family:Verdana;
	font-size: 11px;
	font-weight: bold;
	text-align: left;
	padding-right: 3px;
	padding-left: 3px;
	padding-top: 4;
	padding-bottom: 4;
	border-right-style: solid;
	border-right-width: 1px;
	border-color: white;
}

.eXtremeTable .odd a, .even a {
	color: Black;
	font-size: 10px;
}

.eXtremeTable .odd td, .eXtremeTable .even td {
	padding-top: 2px;
	padding-right: 3px;
	padding-bottom: 2px;
	padding-left: 3px;
	vertical-align: top;
	font-family:Verdana;
	font-size: 10px;
}

.eXtremeTable .odd {
	background-color: #FFFFFF;
}

.eXtremeTable .even {
  	background-color: #dfe4e8;
}

.eXtremeTable .highlight td {
	color: black;
	font-size: 10px;
	padding-top: 2px;
	padding-right: 3px;
	padding-bottom: 2px;
	padding-left: 3px;
	vertical-align: middle;
	background-color: #fdecae;
}

.eXtremeTable .highlight a, .highlight a {
	color: black;
	font-size: 10px;
}

.eXtremeTable .toolbar {
	background-color: #F4F4F4;
	font-family:Verdana;
	font-size: 9px;
	margin-right: 1px;
	border-right: 1px solid silver;
	border-left: 1px solid silver;
	border-top: 1px solid silver;
	border-bottom: 1px solid silver;
}

.eXtremeTable .toolbar td {
	color: #444444;
	padding: 0px 3px 0px 3px;
	text-align:center;
}

.eXtremeTable .separator {
	width: 7px;
}

.eXtremeTable .statusBar {
	background-color: #F4F4F4;
	font-family:Verdana;
	font-size: 10px;
}

.eXtremeTable .filterButtons {
	background-color: #efefef;
	text-align: right;
}

.eXtremeTable .title {
	color: #444444;
	font-weight: bold;
	font-family:Verdana;
	font-size: 15px;
	vertical-align: middle;
}

.eXtremeTable .title span {
	margin-left: 7px;
}

.eXtremeTable .formButtons {
	display: block;
	margin-top: 10px;
	margin-left: 5px;
}

.eXtremeTable .formButton {
	cursor: pointer;
	font-family:Verdana;
	font-size:10px;
	font-weight: bold;
	background-color: #308dbb;
	color: white;
	margin-top: 5px;
	border: outset 1px #333;
	vertical-align: middle;
}

.eXtremeTable .tableTotal {
	background-color: #FFFFFF;
	border-top: solid 1px Silver;	
}

.eXtremeTable .tableTotalEmpty {
	background-color: #FFFFFF;	
}

